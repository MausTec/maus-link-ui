{
  "name": "nogasm-ui",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://link.maustec.io/",
  "main": "electron/main.js",
  "dependencies": {
    "@maustec/react-buttplug": "0.0.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^28.1.0",
    "@types/node": "^17.0.38",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.5",
    "buttplug": "^0.13.2",
    "byte-size": "^7.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "materialize-css": "^1.0.0-rc.2",
    "react": "^16.13.1",
    "react-charts": "^2.0.0-beta.7",
    "react-dom": "^16.13.1",
    "react-materialize": "^3.9.3",
    "react-measure": "^2.5.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0",
    "react-websocket": "^2.1.0",
    "recharts": "^1.8.5",
    "typescript": "^3.9.10"
  },
  "scripts": {
    "start-react": "react-scripts start",
    "start-forge": "electron-forge start",
    "start": "electron-forge start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "pack": "electron-builder --dir",
    "dist": "npm run build && npm run make",
    "make": "electron-forge make",
    "setup-dist": "npx @electron-forge/cli import",
    "rebuild": "./node_modules/.bin/electron-reubild",
    "package": "electron-forge package"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "io.maustec.maus-link.desktop",
    "mac": {
      "category": "io.maustec.maus-link.desktop"
    }
  },
  "postinstall": "electron-builder install-app-deps",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^11.0.4",
    "gh-pages": "^3.1.0",
    "node-sass": "^5.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "link"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
